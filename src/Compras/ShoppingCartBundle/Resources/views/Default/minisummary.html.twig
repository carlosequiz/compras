{% if items is empty %}
    Your cart is empty!!
{% else %}
    
{% for item in items %}
    <li>
        <a href="#"><img width="50" height="50" src="{{  asset(directorio_imagenes ~ item.producto.rutaFoto) }}" />
        {{ item.producto }}</a>
    </li>
{% endfor %}
    <li>
        <div id="browse_app" align="center">
            <a class="btn btn-large btn-info" href="{{ path('shopping_cart_summary') }}">Ver Carrito</a>
        </div>
    </li>
    
{% endif %}